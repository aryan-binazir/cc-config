{
  "permissions": {
    "allow": [
      "Bash(*)",
      "WebFetch",
      "WebSearch",
      "Read",
      "Glob",
      "Grep",
      "LS",
      "NotebookRead",
      "TodoRead",
      "Write",
      "Edit",
      "MultiEdit",
      "Create"
    ],
    "deny": [
      "Bash(git push:*)",
      "Bash(git push)",
      "Bash(rm -rf *)",
      "Bash(dd if=*)",
      "Bash(rm -rf /)",
      "Bash(curl * | sh)",
      "Bash(wget * | sh)",
      "Bash(mkfs.*)",
      "Bash(docker rm -f *)"
    ]
  }
}